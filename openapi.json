  "components": {
    "schemas": {
      "Dagpengegrunnlag": {
        "type": "object",
        "required": [
          "avkortet",
          "uavkortet"
        ],
        "properties": {
          "avkortet": {
            "type": "integer",
            "example": 342352,
            "description": "Fiks ferdig utregnet grunnlag, avkortet til 6G"
          },
          "uavkortet": {
            "type": "integer",
            "example": 342352,
            "description": "Fiks ferdig utregnet grunnlag uten å være avkortet til 6G"
          }
        }
      },
      "DagpengegrunnlagBeregning": {
        "allOf": [
          {
            "$ref": "#/components/schemas/GrunnlagOgSatsSubsumsjon"
          },
          {
            "type": "object",
            "required": [
              "parametere",
              "resultat"
            ],
            "properties": {
              "parametere": {
                "$ref": "#/components/schemas/DagpengegrunnlagResultatParametere"
              },
              "resultat": {
                "$ref": "#/components/schemas/DagpengegrunnlagResultat"
              },
              "inntekt": {
                "type": "array",
                "description": "Hvis manuelt grunnlag er satt så vil tom Inntekt returneres.",
                "items": {
                  "$ref": "#/components/schemas/Inntekt"
                }
              },
              "inntektManueltRedigert": {
                "type": "boolean",
                "description": "Indikerer at inntekten er redigert manuelt av saksbehandler",
                "example": false
              },
              "inntektAvvik": {
                "type": "boolean",
                "description": "Indikerer at inntektet inneholder avvik som ikke er med i beregningen",
                "example": true
              }
            }
          }
        ]
      },
      "DagpengegrunnlagInnParametere": {
        "type": "object",
        "required": [
          "aktorId",
          "vedtakId",
          "beregningsdato"
        ],
        "properties": {
          "aktorId": {
            "type": "string",
            "description": "Identifikasjon av bruker",
            "example": "01019955667"
          },
          "vedtakId": {
            "type": "integer",
            "description": "Identifikasjon av dagpengevedtak",
            "format": "int32",
            "example": 1234
          },
          "beregningsdato": {
            "type": "string",
            "description": "Dato beregning skal utføres på bakgrunn av. Regelverk på aktuelt tidspunkt skal benyttes. Inntekter i henhold til gitt dato skal benyttes.",
            "example": "2019-01-11"
          },
          "harAvtjentVerneplikt": {
            "type": "boolean",
            "default": false,
            "description": "Om bruker har avtjent verneplikt minst 3 av de siste 12 måneder."
          },
          "oppfyllerKravTilFangstOgFisk": {
            "type": "boolean",
            "default": false,
            "description": "Om bruker oppfyller særvilkårene for fangst og fisk."
          },
          "antallBarn": {
            "type": "integer",
            "default": 0,
            "description": "Antall barn bruker søker barnetillegg for"
          },
          "grunnlag": {
            "type": "integer",
            "nullable": true,
            "description": "Er grunnlaget allerede kjent, kan det manuelt overstyres ved å sette sum her"
          }
        }
      },
      "DagpengegrunnlagResultat": {
        "type": "object",
        "required": [
          "grunnlag",
          "sats",
          "beregningsregel",
          "benyttet90ProsentRegel"
        ],
        "properties": {
          "grunnlag": {
            "$ref": "#/components/schemas/Dagpengegrunnlag"
          },
          "sats": {
            "$ref": "#/components/schemas/Sats"
          },
          "beregningsregel": {
            "type": "string",
            "example": "ORDINAER_ETTAAR",
            "description": "Beskriver hvilken regel som er lagt til grunn. ORDINAER_OVER_6G_SISTE_2019 og ORDINAER_OVER_6G_3SISTE_2019 betyr at grunnlag er beregnet av henholdsvis siste 12 eller 36 måneders inntekt. MANUELL_OVER_6G og MANUELL_UNDER_6G betyr henholdsvis om manuelt grunnlag ble kuttet ned fordi det oversteg 6G eller ikke. VERNEPLIKT betyr at grunnlag ble satt utifra regler for verneplikt.",
            "enum": [
              "ORDINAER_OVER_6G_SISTE_2019",
              "ORDINAER_OVER_6G_3SISTE_2019",
              "MANUELL_UNDER_6G",
              "MANUELL_OVER_6G",
              "VERNEPLIKT"
            ]
          },
          "benyttet90ProsentRegel": {
            "type": "boolean",
            "example": false,
            "description": "Settes til true dersom det totalt beregnede barnetillegget overstiger 90 % av dagpengegrunnlaget. Barnetillegget er derfor redusert til 90 % av barnetillegget. Dette gjenspeiles i ukessatsene."
          }
        }
      },
      "DagpengegrunnlagResultatParametere": {
        "type": "object",
        "required": [
          "aktorId",
          "vedtakId",
          "beregningsdato"
        ],
        "properties": {
          "aktorId": {
            "type": "string",
            "description": "Identifikasjon av bruker",
            "example": "01019955667"
          },
          "vedtakId": {
            "type": "integer",
            "description": "Identifikasjon av dagpengevedtak",
            "format": "int32",
            "example": 1234
          },
          "beregningsdato": {
            "type": "string",
            "description": "Dato beregning skal utføres på bakgrunn av. Regelverk på aktuelt tidspunkt skal benyttes. Inntekter i henhold til gitt dato skal benyttes.",
            "example": "2019-01-11"
          },
          "inntektsId": {
            "type": "string",
            "format": "ulid",
            "description": "Identifiserer hvilke inntekter som lå til grunn for beregningen.",
            "example": "01D2FB328P06WD808P22N9CFY5"
          },
          "harAvtjentVerneplikt": {
            "type": "boolean",
            "default": false,
            "description": "Om bruker har avtjent verneplikt minst 3 av de siste 12 måneder."
          },
          "oppfyllerKravTilFangstOgFisk": {
            "type": "boolean",
            "default": false,
            "description": "Om bruker oppfyller særvilkårene for fangst og fisk."
          },
          "antallBarn": {
            "type": "integer",
            "default": 0,
            "description": "Antall barn bruker søker barnetillegg for"
          },
          "grunnlag": {
            "type": "integer",
            "nullable": true,
            "description": "Er grunnlaget allerede kjent, kan det manuelt overstyres ved å sette sum her"
          }
        }
      },
      "GrunnlagOgSatsSubsumsjon": {
        "type": "object",
        "required": [
          "satsSubsumsjonsId",
          "opprettet",
          "utfort"
        ],
        "properties": {
          "grunnlagSubsumsjonsId": {
            "type": "string",
            "example": "01ARZ3NDEKTSV4RRFFQ69G5FAV",
            "description": "Unik ID for denne beregningen - ULID https://github.com/ulid/spec"
          },
          "satsSubsumsjonsId": {
            "type": "string",
            "example": "01ARZ3NDEKTSV4RRFFQ69G5FAV",
            "description": "Unik ID for denne beregningen - ULID https://github.com/ulid/spec"
          },
          "opprettet": {
            "type": "string",
            "description": "Dato for når kjøringen ble opprettet",
            "example": "2018-12-26T14:42:09Z"
          },
          "utfort": {
            "type": "string",
            "description": "Dato for når kjøringen ble oppdatert",
            "example": "2018-12-26T14:43:12Z"
          }
        }
      },
      "HttpProblem": {
        "type": "object",
        "required": [
          "type",
          "title",
          "status"
        ],
        "properties": {
          "type": {
            "type": "string",
            "example": "about:blank",
            "description": "URI referanse [RFC3986] som identifiserer problemtypen. Standard verdi er \"about:blank\""
          },
          "title": {
            "type": "string",
            "example": "En feil har oppstått ved beregningen",
            "description": "Tittel, kort oppsummering av problemtypen."
          },
          "status": {
            "type": "integer",
            "example": 500,
            "description": "HTTP status-kode [RFC7231] generert av tjenesten der feilen oppstod."
          },
          "detail": {
            "type": "string",
            "example": "Beregningen feilet - inntektskomponenten svarte ikke",
            "description": "Menneskelesbar beskrivelse spesifikt til dette problemet"
          },
          "instance": {
            "type": "string",
            "example": "/inntekt/1234",
            "description": "URI referanse [RFC3986] som identifiserer den spesifikke ressursen der problemet oppstod"
          }
        }
      },
      "Inntekt": {
        "type": "object",
        "required": [
          "inntektsPeriode",
          "inntekt",
          "periode",
          "inneholderNaeringsinntekter"
        ],
        "properties": {
          "inntektsPeriode": {
            "$ref": "#/components/schemas/InntektsPeriode"
          },
          "inntekt": {
            "type": "integer",
            "example": 200000,
            "description": "Inntekt for perioden"
          },
          "periode": {
            "type": "integer",
            "description": "Siste-, nestsiste- eller tredjesiste-periode, der 1 vil si siste periode nærmest beregningsdato, og 3 vil si tredje siste periode lengst tilbake i tid.",
            "example": 1,
            "enum": [
              1,
              2,
              3
            ]
          },
          "inneholderNaeringsinntekter": {
            "type": "boolean",
            "example": false,
            "description": "Angir om inntektene som har inngått i vurderingen inneholder næringsinntekter."
          },
          "andel": {
            "type": "integer",
            "example": 2222,
            "description": "Andel av inntekt siste-, nestsiste- eller tredjesiste-periode"
          }
        }
      },
      "InntektsPeriode": {
        "type": "object",
        "description": "Et uttrykk for en periode med inntekt.",
        "required": [
          "forsteMaaned",
          "sisteMaaned"
        ],
        "properties": {
          "foersteMaaned": {
            "type": "string",
            "example": "2018-01",
            "description": "Første måned kronologisk i perioden"
          },
          "sisteMaaned": {
            "type": "string",
            "example": "2019-01",
            "description": "Siste måned kronologisk i perioden"
          }
        }
      },
      "InntjeningsperiodeParametre": {
        "type": "object",
        "required": [
          "aktorId",
          "vedtakId",
          "beregningsdato",
          "inntektsId"
        ],
        "properties": {
          "aktorId": {
            "type": "string",
            "description": "Identifikasjon av bruker",
            "example": "01938420203"
          },
          "vedtakId": {
            "type": "integer",
            "description": "Identifikasjon av dagpengevedtak",
            "format": "int32",
            "example": 12345678
          },
          "beregningsdato": {
            "type": "string",
            "description": "Dato beregning skal utføres på bakgrunn av. Regelverk på aktuelt tidspunkt skal benyttes. Inntekter i henhold til gitt dato skal benyttes.",
            "example": "2019-01-11"
          },
          "inntektsId": {
            "type": "string",
            "format": "ulid",
            "description": "Identifiserer hvilke inntekter som lå til grunn for tidligere? beregningen.",
            "example": "01D2FB328P06WD808P22N9CFY5"
          }
        }
      },
      "InntjeningsperiodeResultat": {
        "type": "object",
        "required": [
          "sammeInntjeningsPeriode",
          "parametere"
        ],
        "properties": {
          "sammeInntjeningsPeriode": {
            "type": "boolean",
            "description": "Indikerer om inntektene fremdeles er gyldige eller ikke for den nye beregningsdatoen.",
            "example": true
          },
          "parametere": {
            "$ref": "#/components/schemas/InntjeningsperiodeParametre"
          }
        }
      },
      "MinsteinntektBeregning": {
        "allOf": [
          {
            "$ref": "#/components/schemas/MinsteinntektOgPeriodeSubsumsjon"
          },
          {
            "type": "object",
            "required": [
              "parametere",
              "resultat",
              "inntekt"
            ],
            "properties": {
              "parametere": {
                "$ref": "#/components/schemas/MinsteinntektResultatParametere"
              },
              "resultat": {
                "$ref": "#/components/schemas/MinsteinntektResultat"
              },
              "inntekt": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/Inntekt"
                }
              },
              "inntektManueltRedigert": {
                "type": "boolean",
                "description": "Indikerer at inntekten er redigert manuelt av saksbehandler",
                "example": false
              },
              "inntektAvvik": {
                "type": "boolean",
                "description": "Indikerer at inntektet inneholder avvik som ikke er med i beregningen",
                "example": true
              }
            }
          }
        ]
      },
      "MinsteinntektInnParametere": {
        "required": [
          "aktorId",
          "vedtakId",
          "beregningsdato"
        ],
        "type": "object",
        "properties": {
          "aktorId": {
            "type": "string",
            "description": "Identifikasjon av bruker",
            "example": "01019955667"
          },
          "vedtakId": {
            "type": "integer",
            "description": "Identifikasjon av dagpengevedtak",
            "format": "int32",
            "example": 1234
          },
          "beregningsdato": {
            "type": "string",
            "description": "Dato beregning skal utføres på bakgrunn av. Regelverk på aktuelt tidspunkt skal benyttes. Inntekter i henhold til gitt dato skal benyttes.",
            "example": "2019-01-11"
          },
