{
  "openapi": "3.0.0",
  "info": {
    "title": "Dagpenger Regel API - Arena Adapter",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "https://dagpenger-regel-api-arena-adapter.nais.adeo.no:{port}/v1",
      "variables": {
        "port": {
          "default": "443",
          "enum": [
            "443",
            "8443"
          ]
        }
      }
    }
  ],
  "tags": [
    {
      "name": "Minsteinntekt",
      "description": "Beregner minsteinntekt og dagpengeperiode etter §4-4 og §4-15",
      "externalDocs": {
        "description": "Folketrygdloven $4-4",
        "url": "https://lovdata.no/lov/1997-02-28-19/%C2%A74-4"
      }
    },
    {
      "name": "Dagpengegrunnlag",
      "description": "Beregner dagpengegrunnlag etter §4-11",
      "externalDocs": {
        "description": "Folketrygdloven $4-11",
        "url": "https://lovdata.no/lov/1997-02-28-19/%C2%A74-11"
      }
    },
    {
      "name": "Inntjeningsperiode",
      "description": "Operasjon for å vurdere om inntektene fremdeles er gyldige for gitt beregningsdatoen."
    }
  ],
  "paths": {
    "/dagpengegrunnlag": {
      "post": {
        "tags": [
          "Dagpengegrunnlag"
        ],
        "summary": "Beregn dagpengegrunnlag",
        "requestBody": {
          "description": "Hvilke parametere som skal brukes for beregningen",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DagpengegrunnlagInnParametere"
              },
              "examples": {
                "grunnlag": {
                  "$ref": "#/components/examples/dagpengegrunnlag-grunnlag"
                },
                "verneplikt": {
                  "$ref": "#/components/examples/dagpengegrunnlag-verneplikt"
                },
                "bareSats": {
                  "$ref": "#/components/examples/dagpengegrunnlag-bareSats"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "En beregning av dagpengegrunnlag og/eller sats",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DagpengegrunnlagBeregning"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input - feilmelding i henhold til [RFC7807] https://tools.ietf.org/html/rfc7807",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HttpProblem"
                },
                "examples": {
                  "400": {
                    "$ref": "#/components/examples/400"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Uautorisert - feilmelding i henhold til [RFC7807] https://tools.ietf.org/html/rfc7807",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HttpProblem"
                },
                "examples": {
                  "401": {
                    "$ref": "#/components/examples/401"
                  }
                }
              }
            }
          },
          "403": {
            "description": "Ikke tilgang - feilmelding i henhold til [RFC7807] https://tools.ietf.org/html/rfc7807",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HttpProblem"
                },
                "examples": {
                  "403": {
                    "$ref": "#/components/examples/403"
                  }
                }
              }
            }
          },
          "500": {
            "description": "System error - feilmelding i henhold til [RFC7807] https://tools.ietf.org/html/rfc7807",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HttpProblem"
                },
                "examples": {
                  "inntektFeil": {
                    "$ref": "#/components/examples/hent-inntekt-feilet"
                  }
                }
              }
            }
          }
        }
      }
    },
