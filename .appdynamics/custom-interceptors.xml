<custom-interceptors>
    <custom-interceptor>
        <interceptor-class-name>com.singularity.KafkaMarkerMethodInterceptor</interceptor-class-name>
        <match-class type="inherits-class">
            <name filter-type="equals">no.nav.dagpenger.streams.River</name>
        </match-class>
        <match-method>
            <name>onPacket</name>
        </match-method>
    </custom-interceptor>
</custom-interceptors>
